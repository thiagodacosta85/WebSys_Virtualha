theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust = 1),
axis.text.y = element_text(angle = 0, vjust = 0.5, hjust = 1)) +
labs(x = "Localidade",
y = "População",
title = "Distribuição Populacional",
subtitle = NULL) +
geom_label(size = 3, fill="gray95")
